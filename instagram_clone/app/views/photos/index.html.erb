<h1>Photos</h1>

<% if @photos.all.any? %>
  <% @photos.all.each do |photo| %>
  <%= link_to image_tag(photo.image.url, class: 'media-object', height: '500', width: '500'), photo_path(photo), id: "photo#{photo.id}" %>
    <h2><%= photo.caption %></h2>
  <% end %>
<% else %>
  <h1>No photos yet</h1>
<% end %>
